{
    "name": "wishknish/knishio-server-reference-laravel",
    "description": "Reference implementation of KnishIO-Server-PHP on the Laravel 9+ PHP framework.",
    "keywords": [
        "knishio",
        "wishkinsh",
        "blockchain",
        "api",
        "graphql",
        "dag"
    ],
    "type": "project",
    "require": {
        "php": "^8.0",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.14",
        "laravel/tinker": "^2.7",
        "wishknish/knishio-server-php": "dev-eugene-teplitsky-dev"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Tests\\": "tests/",
            "WishKnish\\KnishIO\\Tests\\": "vendor/wishknish/knishio-server-php/tests/",
            "WishKnish\\KnishIO\\Client\\Tests\\": "vendor/wishknish/knishio-client-php/tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:wishknish/knishio-server-php.git"
        },
        {
            "type": "github",
            "url": "https://github.com/evias/PHP-SHA3-Streamable"
        }
    ]
}
